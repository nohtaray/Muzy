<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <filter>
        <filter-name>CharacterEncodingFilter</filter-name>
        <filter-class>filter.CharacterEncodingFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>CharacterEncodingFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <servlet>
        <servlet-name>LoginWithGoogleServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.LoginWithGoogleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SignUpWithGoogleServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.SignUpWithGoogleServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginWithPasswordServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.LoginWithPasswordServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewMusicServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.NewMusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>YoutubeVideosServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.YoutubeVideosServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>MusicServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.MusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingMusicServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.RankingMusicServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RankingArtistServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.RankingArtistServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IndexServlet</servlet-name>
        <servlet-class>jp.ac.jec.jz.gr03.servlet.IndexServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LoginWithGoogleServlet</servlet-name>
        <url-pattern>/LoginWithGoogleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SignUpWithGoogleServlet</servlet-name>
        <url-pattern>/SignUpWithGoogleServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginWithPasswordServlet</servlet-name>
        <url-pattern>/LoginWithPasswordServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewMusicServlet</servlet-name>
        <url-pattern>/NewMusicServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>YoutubeVideosServlet</servlet-name>
        <url-pattern>/YoutubeVideosServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MusicServlet</servlet-name>
        <url-pattern>/MusicServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingMusicServlet</servlet-name>
        <url-pattern>/RankingMusicServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RankingArtistServlet</servlet-name>
        <url-pattern>/RankingArtistServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IndexServlet</servlet-name>
        <url-pattern>/IndexServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
